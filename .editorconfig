# EditorConfig is awesome: http://EditorConfig.org

root = true

[*]
end_of_line              = lf
insert_final_newline     = true
trim_trailing_whitespace = true

[*.php]
indent_style = space
indent_size  = 4
charset      = utf-8

[*.js, *.css]
indent_style = space
indent_size  = 2
charset      = utf-8
